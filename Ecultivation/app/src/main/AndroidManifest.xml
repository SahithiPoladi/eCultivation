<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sahi.registration">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-feature android:name="android.hardware.Camera2"  android:required="true"></uses-feature>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Ecultivation"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".FarmerActivity"/>
        <activity android:name=".EloginActivity"/>
        <activity android:name=".EregisterActivity"/>
        <activity android:name=".VregisterActivity"/>
        <activity android:name=".VloginActivity"/>
        <activity android:name=".VillageActivity"/>
        <activity android:name=".ImageActivity"/>
        <activity android:name=".CheckActivity"/>
        <activity android:name=".SearchActivity"/><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>

    </application>

</manifest>